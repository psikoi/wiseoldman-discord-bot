{
  "name": "discord-bot",
  "version": "1.1.0",
  "description": "A Discord bot for the Wise Old Man projects (https://github.com/wise-old-man/wise-old-man/)",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "dev": "docker-compose up --build",
    "watch": "cross-env NODE_ENV=development prisma migrate dev && ts-node-dev --poll --exit-child --respawn --transpile-only --ignore-watch node_modules src/index.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "osrs",
    "runescape",
    "oldschool",
    "wiseoldman",
    "wise-old-man"
  ],
  "author": "Psikoi",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.155",
    "@types/node": "^16.11.47",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "prisma": "^4.6.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.4.1",
    "@prisma/client": "^4.6.1",
    "@sapphire/discord.js-utilities": "^4.8.0",
    "@wise-old-man/utils": "^2.0.0-beta.8",
    "axios": "^0.19.2",
    "canvas": "^2.6.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "discord-api-types": "^0.27.2",
    "discord.js": "^13.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.26.0"
  },
  "engines": {
    "node": ">=16.9"
  }
}
